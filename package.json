{
  "name": "frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run build-themes && ng serve",
    "build": "npm run build-themes && ng build",
    "build-prod": "npm run build-themes && ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "build-themes": "bash ./tools/build-themes.sh",
    "e2e": "ng e2e --protractor-config=e2e/protractor-ci.conf.js --prod",
    "dev:ssr": "npm run build-themes && ng run frontend:serve-ssr",
    "serve:ssr": "node dist/frontend/server/main.js",
    "build:ssr": "ng build && ng run frontend:server",
    "build-prod:ssr": "ng build --prod && ng run frontend:server:production",
    "prerender": "ng run frontend:prerender"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^11.1.2",
    "@angular/cdk": "^11.1.2",
    "@angular/common": "^11.1.2",
    "@angular/compiler": "^11.1.2",
    "@angular/core": "^11.1.2",
    "@angular/flex-layout": "^11.0.0-beta.33",
    "@angular/forms": "^11.1.2",
    "@angular/material": "^11.1.2",
    "@angular/platform-browser": "^11.1.2",
    "@angular/platform-browser-dynamic": "^11.1.2",
    "@angular/platform-server": "^11.1.2",
    "@angular/router": "^11.1.2",
    "@auth0/angular-jwt": "^5.0.2",
    "@nguniversal/express-engine": "^11.1.2",
    "@sentry/browser": "^6.2.5",
    "compression": "^1.7.4",
    "core-js": "^3.8.3",
    "domino": "^2.1.6",
    "express": "^4.15.2",
    "lodash": "^4.17.21",
    "ngx-markdown": "^11.1.3",
    "ngx-monaco-editor": "^9.0.0",
    "numeral": "^2.0.6",
    "query-string": "^6.13.8",
